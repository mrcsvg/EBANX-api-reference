{
  "name": "ebanx-payments",
  "version": "1.0.0",
  "dependencies": {
    "@redocly/openapi-cli": "^0.8.3",
    "openapi-cli": "0.0.2",
    "redoc-cli": "^0.9.6"
  },
  "private": true,
  "scripts": {
    "start": "openapi preview-docs",
